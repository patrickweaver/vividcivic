<ul class="postlist">
{{#each collections.postsReversed }}
  {{#if this.data.title}}
  <li class="post">
      
    {{> postHeader this.data url=this.url templateContent=this.templateContent index=true}}
    
    <p>
      {{ previewText this.templateContent }}… <a href="{{ this.url }}">Read More →</a>
    </p>      
  </li>
  {{/if}}
{{/each}}
</ul>
